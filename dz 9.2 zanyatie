# класс живность
class Animal:
    def sound(self):
        return "Животное издает звук"

class Dog(Animal):
    def sound(self):
        return "Гав-гав"

class Cat(Animal):
    def sound(self):
        return "Мяу"

class Cow(Animal):
    def sound(self):
        return "Муу"

# список животных
animals = [Dog(), Cat(), Cow()]

for animal in animals:
    print(animal.sound())
